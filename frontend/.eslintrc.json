{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:vue/essential",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": [
            "error"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            "tab"
        ],
        "notabs": "off",
        "comma-dangle": [
            "error",
            "never"
        ],
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "space-before-blocks": [
            "error",
            "always"
        ]
    }
}